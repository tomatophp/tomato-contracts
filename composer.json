{
    "name": "tomatophp/tomato-contracts",
    "type": "library",
    "description": "Build Contract Templates and use it for Tomato PMS",
    "keywords": [
        "php",
        "laravel",
        "template"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TomatoPHP\\TomatoContracts\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TomatoPHP\\TomatoContracts\\TomatoContractsServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Fady Mondy",
            "email": "info@3x1.io"
        }
    ],
    "require": {
        "php": "^8.1",
        "tomatophp/tomato-pms": "^1.0",
        "tomatophp/tomato-crm": "^1.1",
        "tomatophp/console-helpers": "^1.1"
    }
}
